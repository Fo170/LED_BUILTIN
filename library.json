{
    "name": "LED_BUILTIN",
    "version": "2.0.3",
    "description": "Non-blocking library for managing built-in LEDs (LED_BUILTIN) on ESP8266 and ESP32 boards with extended GPIO support, RGB LED support, and advanced blinking functions using millis()",
    "keywords": [
    "led",
    "builtin",
    "esp8266",
    "esp32",
    "gpio",
    "blink",
    "non-blocking",
    "millis",
    "duty-cycle",
    "rgb",
    "neopixel",
    "ws2812",
    "heltec",
    "ttgo",
    "adafruit",
    "sparkfun",
    "m5stack",
    "wemos",
    "lolin",
    "esp32-c3",
    "esp32-s2",
    "esp32-s3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Fo170/LED_BUILTIN.git"
  },
  "authors": [
    {
      "name": "Fo170",
      "email": "olivier.fournet@free.fr",
      "url": "https://github.com/Fo170",
      "maintainer": true
    }
  ],
  "license": "GPL-3.0-only",
  "homepage": "https://github.com/Fo170/LED_BUILTIN",
  "frameworks": "arduino",
  "platforms": [
    "espressif8266",
    "espressif32"
  ],
  "headers": "LED_BUILTIN.h",
  "examples": [
    {
      "name": "Basic Non-Blocking Example",
      "base": "examples/Basic_Example"
    }
  ],
  "export": {
    "exclude": [
      ".github",
      ".git",
      ".gitignore",
      "extras",
      "test",
      "docs",
      "*.md",
      "*.txt",
      ".vscode",
      ".DS_Store"
    ]
  },
  "build": {
    "flags": [
      "-I include",
      "-DLED_BUILTIN_VERSION=2.0.3"
    ],
    "libArchive": false
  }
}
